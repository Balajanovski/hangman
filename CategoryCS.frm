Private Sub Letter_Click(Index As Integer)
    If Includes(KeyAlpha(Index), AlreadyGuessed)(0) < 0 Then
        If Includes(KeyAlpha(Index), ArrayCS(IndexCurr))(0) >= 0 Then
            For IterA = 0 To UBound(Includes(KeyAlpha(Index), ArrayCS(IndexCurr)))
                If Includes(KeyAlpha(Index), ArrayCS(IndexCurr))(IterA) < 0 Then
                    Exit For
                Else
                    Word(Includes(KeyAlpha(Index), ArrayCS(IndexCurr))(IterA)).FontSize = 55
                    Word(Includes(KeyAlpha(Index), ArrayCS(IndexCurr))(IterA)).FontUnderline = True
                    Word(Includes(KeyAlpha(Index), ArrayCS(IndexCurr))(IterA)).Caption = KeyAlpha(Index)
                    Word(Includes(KeyAlpha(Index), ArrayCS(IndexCurr))(IterA)).Top = Word(Includes(KeyAlpha(Index), ArrayCS(IndexCurr))(IterA)).Top - 10
                End If
            Next
            Call Append(KeyAlpha(Index), False)
        Else
            TurnsLeft = TurnsLeft - 1
            Call DrawMan(TurnsLeft)
            Call Append(KeyAlpha(Index), True)
            End If
    End If
    For IterA = 0 To 25
        If Includes(KeyAlpha(IterA), AlreadyGuessed)(0) >= 0 Then
            Letter(IterA).Enabled = False
            Letter(IterA).FontBold = False
            Letter(IterA).FontSize = 10
        End If
    Next
    If TurnsLeft = 2 And Difficulty < 0 Then
        ShowHint.Visible = True
    End If
    PrintArr (AlreadyGuessed)
    If TurnsLeft = 0 Then
        MsgBox "You Lose. The word was " + Join(IndexCurr)
        Call Reset
        Call EndGame
    ElseIf CheckWord(IndexCurr) = True Then
        If Round = 4 Then
            MsgBox "You Win!"
            Call EndGame
        Else
            MsgBox "Well done! Time for Word" + Str(Round + 1)
            Round = Round + 1
            IndexCurr = Random(0, 9)
            Do While IncludesNum(IndexCurr, IndicesDone) = True
                IndexCurr = Random(0, 9)
            Loop
            Call AppendNum(IndexCurr, IndicesDone)
            Call NewRound
            Call MakeVisible(IndexCurr)
        End If
    End If
End Sub


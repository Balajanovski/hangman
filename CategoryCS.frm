'ArrayHints = Array("Brain of a computer", "Lowest value of memory", "Value equal to 8 bits", "Clause in if-statement", "A template for creating objects", "A trigger for a certain activity", "Storage of a computer", "Data type holding characters", "A resuable block of code", "A link of different devices")
Dim ArrayCS() As Variant
Dim ArrayHints() As Variant
Dim KeyAlpha() As Variant
Dim AlreadyGuessed(0 To 25) As Variant
Dim Incorrect(0 To 25) As Variant
Dim IndicesDone() As Variant
Dim TurnsLeft As Integer
Dim Round As Integer
Dim IndexCurr As Integer
Dim Including As Integer
Dim IterA As Integer
Dim StartLeft As Integer
Dim TimeLeft As Integer
Dim Difficulty As Integer
Dim FlagTimed As Boolean
Dim FlagSelect As Boolean

Private Sub EndGame()
    Dim Result As VbMsgBoxResult
    Result = MsgBox("Yes to Retry, No to Quit", vbYesNo, "Quit Game")
    If Result = vbYes Then
        PickScreen.Show
        Unload Me
    Else
        Unload Me
    End If
End Sub

Private Sub PrintArr(ByVal arr As Variant)
    Dim Iter As Integer
    For Iter = 0 To 25
        If arr(Iter) = "" Then
            Exit For
        Else
            GuessArr(Iter).Caption = arr(Iter)
            If Includes(arr(Iter), Incorrect)(0) >= 0 Then
                GuessArr(Iter).ForeColor = &HFF&
            End If
        End If
    Next
End Sub


Private Function CheckWord(ByVal Index As Integer) As Boolean
    CheckWord = True
    Dim Iter As Integer
    For Iter = 0 To UBound(ArrayCS(Index))
        If Word(Iter).Caption = "_" Then
            CheckWord = False
        End If
    Next
End Function

Private Sub Form_Load()
    ArrayCS = Array(Array("c", "p", "u"), Array("b", "i", "t"), Array("b", "y", "t", "e"), Array("e", "l", "s", "e"), Array("c", "l", "a", "s", "s"), Array("e", "v", "e", "n", "t"), Array("m", "e", "m", "o", "r", "y"), Array("s", "t", "r", "i", "n", "g"), Array("r", "o", "u", "t", "i", "n", "e"), Array("n", "e", "t", "w", "o", "r", "k"))
    ArrayHints = Array("Brain of a computer", "Lowest value of memory", "Value equal to 8 bits", "Clause in if-statement", "A template for creating objects", "A trigger for a certain activity", "Storage of a computer", "Data type holding characters", "A resuable block of code", "A link of different devices")
    KeyAlpha = Array("q", "w", "e", "r", "t", "y", "u", "i", "o", "p", "a", "s", "d", "f", "g", "h", "j", "k", "l", "z", "x", "c", "v", "b", "n", "m")
    IndicesDone = Array(-1, -1, -1, -1)
    For IterA = 0 To 25
        Letter(IterA).Caption = UCase(KeyAlpha(IterA))
        Letter(IterA).FontName = "Courier New"
        Letter(IterA).FontBold = True
        Letter(IterA).FontSize = 12
        GuessArr(IterA).FontSize = 11
    Next
    StartLeft = 2940
    Hint.Visible = False
    Difficulty = Val(PickScreen.LabelDifficulty.Caption)
    FlagTimed = PickScreen.LabelTimed.Caption
    If FlagTimed = "True" Then
        Timer1.Enabled = True
        Timer1.Interval = 1000
        If Difficulty = 1 Then
            TimeLeft = 60
        ElseIf Difficulty = 0 Then
            TimeLeft = 90
        ElseIf Difficulty = -1 Then
            TimeLeft = 120
        End If
        Time.Caption = "Seconds Remaining:" + Str(TimeLeft)
    Else
        Timer1.Enabled = False
        Time.Visible = False
    End If
    Label1.Caption = "Already Guessed:"
    Label1.FontUnderline = True
    ShowHint.Visible = False
    Round = 1
    RoundIdentifier.Caption = "Round:" + Str(Round)
    Call Reset
    IndexCurr = Random(0, 9)
    Call AppendNum(IndexCurr, IndicesDone)
    Call MakeVisible(IndexCurr)
    Call DrawMan(TurnsLeft)
End Sub

Private Sub Reset()
    Dim Iter As Integer
    For Iter = 0 To 6
        Word(Iter).Left = StartLeft + 720 * Iter
        Word(Iter).Top = 2340
        Word(Iter).Height = 1695
        Word(Iter).Width = 615
        Word(Iter).FontSize = 54
        Word(Iter).Caption = "_"
        Word(Iter).FontUnderline = False
        Word(Iter).Visible = False
    Next
    TurnsLeft = 6
    Head.Visible = False
    Torso.Visible = False
    Leg1.Visible = False
    Leg2.Visible = False
    Hand1.Visible = False
    Hand2.Visible = False
    For Iter = 0 To 25
        AlreadyGuessed(Iter) = ""
        Incorrect(Iter) = ""
        GuessArr(Iter).Caption = ""
        GuessArr(Iter).ForeColor = &H8000000B
    Next
End Sub

Private Sub NewRound()
    Dim Iter As Integer
    For Iter = 0 To 6
        Word(Iter).Caption = "_"
        Word(Iter).Top = 2340
        Word(Iter).FontUnderline = False
        Word(Iter).FontSize = 54
        Word(Iter).Visible = False
    Next
    For Iter = 0 To 25
        AlreadyGuessed(Iter) = ""
        Incorrect(Iter) = ""
        GuessArr(Iter).Caption = ""
        GuessArr(Iter).ForeColor = &H8000000B
        Letter(Iter).Caption = UCase(KeyAlpha(Iter))
        Letter(Iter).FontName = "Courier New"
        Letter(Iter).FontBold = True
        Letter(Iter).FontSize = 12
        Letter(Iter).Enabled = True
    Next
    If Difficulty = -1 Then
        TurnsLeft = TurnsLeft + 2
    ElseIf Diffifulty = 0 Then
        TurnsLeft = TurnsLeft + 1
    End If
    If TurnsLeft > 6 Then
        TurnsLeft = 6
    End If
    Call DrawMan(TurnsLeft)
    Hint.Caption = ArrayHints(IndexCurr)
    If TurnsLeft > 2 Then
        ShowHint.Visible = False
    End If
    RoundIdentifier.Caption = "Round:" + Str(Round)
End Sub

Private Function Random(ByVal StartIndex As Integer, ByVal EndIndex As Integer)
    Math.Randomize
    Random = Int(Math.Rnd * (EndIndex - StartIndex + 1)) + StartIndex
End Function

Private Sub Timer1_Timer()
    TimeLeft = TimeLeft - 1
    Time.Caption = "Seconds Remaining:" + Str(TimeLeft)
    If TimeLeft = 0 Then
        MsgBox "Time Up"
        Timer1.Enabled = False
        Call EndGame
    End If
End Sub

Private Sub MakeVisible(ByVal Index As Integer)
    Dim Iter As Integer
    For Iter = 0 To UBound(ArrayCS(Index))
        Word(Iter).Visible = True
    Next
End Sub

Private Function Includes(ByVal Letter As String, ByRef arr As Variant) As Integer()
    Dim temparr(6) As Integer
    Dim Iter As Integer
    For Iter = 0 To 6
        temparr(Iter) = -1
    Next
    For Iter = 0 To UBound(arr)
        If arr(Iter) = Letter Then
            Call AppendNum(Iter, temparr)
        End If
    Next
    Includes = temparr
End Function

Private Function IncludesNum(ByVal Number As Integer, ByRef arr As Variant) As Boolean
    Dim Iter As Integer
    IncludesNum = False
    For Iter = 0 To UBound(arr)
        If arr(Iter) = Number Then
            IncludesNum = True
        End If
    Next
End Function

Private Function Join(ByVal Index As Integer) As String
    Dim Result As String
    Dim Iter As Integer
    Result = ""
    For Iter = 0 To UBound(ArrayCS(Index))
        Result = Result + ArrayCS(Index)(Iter)
    Next
    Join = Result
End Function

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
    Dim Selected As String
    FlagSelect = True
    Select Case KeyCode
        Case vbKeyA
            Selected = "a"
        Case vbKeyB
            Selected = "b"
        Case vbKeyC
            Selected = "c"
        Case vbKeyD
            Selected = "d"
        Case vbKeyE
            Selected = "e"
        Case vbKeyF
            Selected = "f"
        Case vbKeyG
            Selected = "g"
        Case vbKeyH
            Selected = "h"
        Case vbKeyI
            Selected = "i"
        Case vbKeyJ
            Selected = "j"
        Case vbKeyK
            Selected = "k"
        Case vbKeyL
            Selected = "l"
        Case vbKeyM
            Selected = "m"
        Case vbKeyN
            Selected = "n"
        Case vbKeyO
            Selected = "o"
        Case vbKeyP
            Selected = "p"
        Case vbKeyQ
            Selected = "q"
        Case vbKeyR
            Selected = "r"
        Case vbKeyS
            Selected = "s"
        Case vbKeyT
            Selected = "t"
        Case vbKeyU
            Selected = "u"
        Case vbKeyV
            Selected = "v"
        Case vbKeyW
            Selected = "w"
        Case vbKeyX
            Selected = "x"
        Case vbKeyY
            Selected = "y"
        Case vbKeyZ
            Selected = "z"
        Case Else
            MsgBox ("Invalid Key")
            FlagSelect = False
    End Select
    If FlagSelect Then
        If Includes(Selected, AlreadyGuessed)(0) < 0 Then
            If Includes(Selected, ArrayCS(IndexCurr))(0) >= 0 Then
                For IterA = 0 To UBound(Includes(Selected, ArrayCS(IndexCurr)))
                    If Includes(Selected, ArrayCS(IndexCurr))(IterA) < 0 Then
                        Exit For
                    Else
                        Word(Includes(Selected, ArrayCS(IndexCurr))(IterA)).FontSize = 55
                        Word(Includes(Selected, ArrayCS(IndexCurr))(IterA)).FontUnderline = True
                        Word(Includes(Selected, ArrayCS(IndexCurr))(IterA)).Caption = Selected
                        Word(Includes(Selected, ArrayCS(IndexCurr))(IterA)).Top = Word(Includes(Selected, ArrayCS(IndexCurr))(IterA)).Top - 10
                    End If
                Next
                Call Append(Selected, False)
            Else
                TurnsLeft = TurnsLeft - 1
                Call DrawMan(TurnsLeft)
                Call Append(Selected, True)
            End If
        End If
    End If
    For IterA = 0 To 25
        If Includes(KeyAlpha(IterA), AlreadyGuessed)(0) >= 0 Then
            Letter(IterA).Enabled = False
            Letter(IterA).FontBold = False
            Letter(IterA).FontSize = 10
        End If
    Next
    PrintArr (AlreadyGuessed)
    If TurnsLeft = 2 And Difficulty < 0 Then
        ShowHint.Visible = True
    End If
    If TurnsLeft = 0 Then
        MsgBox "You Lose. The word was " + Join(IndexCurr)
        Call Reset
        Call EndGame
    ElseIf CheckWord(IndexCurr) = True Then
        If Round = 4 Then
            MsgBox "You Win!"
            Call EndGame
        Else
            MsgBox "Well done! Time for Word" + Str(Round + 1)
            Round = Round + 1
            IndexCurr = Random(0, 9)
            Do While IncludesNum(IndexCurr, IndicesDone) = True
                IndexCurr = Random(0, 9)
            Loop
            Call AppendNum(IndexCurr, IndicesDone)
            Call NewRound
            Call MakeVisible(IndexCurr)
        End If
    End If
End Sub

Private Sub Append(ByVal Letter As String, ByVal Wrong As Boolean)
    Dim Iter As Integer
    For Iter = 0 To 25
        If AlreadyGuessed(Iter) = "" Then
            AlreadyGuessed(Iter) = Letter
            Exit For
        End If
    Next
    If Wrong Then
        For Iter = 0 To 25
            If Incorrect(Iter) = "" Then
                Incorrect(Iter) = Letter
                Exit For
            End If
        Next
    End If
End Sub

Private Sub AppendNum(ByVal Number As Integer, ByRef arr As Variant)
    Dim Iter As Integer
    For Iter = 0 To UBound(arr)
        If arr(Iter) = -1 Then
            arr(Iter) = Number
            Exit For
        End If
    Next
End Sub


Private Sub DrawMan(ByVal Turns As Integer)
    If Turns >= 6 Then
        Head.Visible = False
        Torso.Visible = False
        Leg1.Visible = False
        Leg2.Visible = False
        Hand1.Visible = False
        Hand2.Visible = False
    ElseIf Turns = 5 Then
        Head.Visible = True
        Torso.Visible = False
        Leg1.Visible = False
        Leg2.Visible = False
        Hand1.Visible = False
        Hand2.Visible = False
    ElseIf Turns = 4 Then
        Torso.Visible = True
        Leg1.Visible = False
        Leg2.Visible = False
        Hand1.Visible = False
        Hand2.Visible = False
    ElseIf Turns = 3 Then
        Leg1.Visible = True
        Leg2.Visible = False
        Hand1.Visible = False
        Hand2.Visible = False
    ElseIf Turns = 2 Then
        Leg2.Visible = True
        Hand1.Visible = False
        Hand2.Visible = False
    ElseIf Turns = 1 Then
        Hand1.Visible = True
        Hand2.Visible = False
    Else
        Hand2.Visible = True
    End If
End Sub

